        "framer-motion": "5.2.1",
        "next": "12.0.4",
        "react": "17.0.2",
        "react-colorful": "5.5.1",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@chakra-ui/cli": "1.6.0",
        "@swc-node/jest": "1.4.1",
        "@testing-library/jest-dom": "5.15.1",
        "@testing-library/react": "12.1.2",
        "@types/eslint": "7.28.2",
